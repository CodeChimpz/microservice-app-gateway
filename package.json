{
  "name": "gateway-service",
  "description": "API gateway that proxies requests from a socket connection to http endpoints on microservices, with endpoint registration from the services in runtime.",
  "version": "1.0.0",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "type": "module",
  "scripts": {
    "docker": "docker compose up --build",
    "dev": "nodemon -L src/index.ts",
    "start": "rm -r dist && tsc && node --trace-warnings ./dist/index.js",
    "test-load": "artillery run test/artillery.io/load_testing.socket.access-ep.yaml -o ./test/artillery.io/output.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeChimpz/microservice-app-gateway.git"
  },
  "author": "codechimpz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodeChimpz/microservice-app-gateway/issues"
  },
  "homepage": "https://github.com/CodeChimpz/microservice-app-gateway#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ioredis": "^5.3.1",
    "ip-range-check": "^0.2.0",
    "knex-db-connector": "^1.0.31",
    "mein-etcd-service-registry": "^1.0.57",
    "mein-winston-logger": "^1.0.7",
    "rate-limiter-flexible": "^2.4.1",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "artillery": "^2.0.0-30",
    "artillery-engine-socketio-v3": "^1.1.2"
  }
}
